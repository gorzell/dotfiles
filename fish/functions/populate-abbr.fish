function populate-abbr
  # Git
  abbr g git
  abbr gs 'git status'
  abbr gd 'git diff'
  abbr gp 'git pull'
  abbr gpu 'git push'
  abbr gcln 'git clone'
  abbr gc 'git commit -v'
  abbr gcmsg 'git commit -m'
  abbr gco 'git checkout'
  abbr gcm 'git checkout master'
  abbr gr 'git remote'
  abbr gb 'git branch'
  abbr ga 'git add'

  # Docker
  abbr dc 'docker-compose'
end
