set commentary
set NERDTree

source ~/.vimrc
source ~/.vim/plug.vim

" Intellij
noremap ga :action ShowIntentionActions<CR>
noremap gd :action GotoDeclaration<CR>
noremap gD :action GotoImplementation<CR>
noremap <leader>ff :action GotoFile<CR>
noremap <leader>fs :action GotoSymbol<CR>
noremap <leader>fg :action SearchEverywhere<CR>
noremap <leader>fc :action GotoClass<CR>
noremap g[ :action GotoPreviousError<CR>
noremap g] :action GotoNextError<CR>
